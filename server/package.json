  "description": "Zero Install, Peer-to-Peer Photo Group Collaboration.",
  "type": "module",
  "main": "app.js",
  "private": true,
  "scripts": {
    "clean": "rimraf ui/*",
    "start": "node app.js",
    "test": "cross-env NODE_ENV=test mocha app.test.js tests/**/*.test.js --timeout 10000 --exit",
    "test:unit": "cross-env NODE_ENV=test mocha tests/unit/**/*.test.js --timeout 10000 --exit",
